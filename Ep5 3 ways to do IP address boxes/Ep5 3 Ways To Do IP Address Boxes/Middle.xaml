<Page x:Class="Basic_Wizard.Middle"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Basic_Wizard"
      mc:Ignorable="d" 
      d:DesignHeight="350" d:DesignWidth="525"
      Title="Middle">

    <Grid x:Name="MiddlePage">
        <Button x:Name="MiddleButtonCancel" Content="Cancel" HorizontalAlignment="Right" Margin="0,0,10.333,10" VerticalAlignment="Bottom" Width="75" IsCancel="True"/>
        <Button x:Name="MiddleButtonNext" Content="Next" HorizontalAlignment="Right" Margin="0,0,90.333,10" VerticalAlignment="Bottom" Width="75"/>
        <Button x:Name="MiddleButtonBack" Content="Back" HorizontalAlignment="Right" Margin="0,0,170.333,10" VerticalAlignment="Bottom" Width="75"/>
        <Border x:Name="MiddleBorder" BorderBrush="Gray" BorderThickness="1" HorizontalAlignment="Left" Height="29" Margin="129,154,0,0" VerticalAlignment="Top" Width="240">
            <TextBox x:Name="MiddleTextBox" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="234" Margin="2,2,0,0"/>
        </Border>
        <TextBlock HorizontalAlignment="Left" Margin="132,129,0,0" TextWrapping="Wrap" Text="Cast IP address box" VerticalAlignment="Top"/>

    </Grid>
</Page>
